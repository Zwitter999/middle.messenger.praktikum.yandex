{
  "env": {
    "browser": true,
    "es2021": true
  },
  "parser": "@typescript-eslint/parser",
  "extends": ["eslint:recommended", "plugin:import/recommended", "prettier"],
  "plugins": ["@typescript-eslint"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "semi": [2, "always"],
    "max-len": [2, 100],
    "@typescript-eslint/no-unused-vars": 2
  }
}
